class Box {
    double w, h, d;

    Box(int a, int b, int c) {  // Parameterized Constructor
        System.out.println("This is a constructor");
        w = a;
        h = b;
        d = c;
        System.out.println("The values of w, h, d are: " + w + ", " + h + ", " + d);
    }

    public static void main(String[] args) {
        Box mybox = new Box(5, 10, 15);  // Creating object using parameterized constructor
    }
}